        android:textSize="12sp"/>

    <TextView
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="505dp"
        android:text="등록 서류:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:textColor="#0F4C81"/>

    <TextView
        android:layout_width="230dp"
        android:layout_height="17dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="530dp"
        android:text="1. 신분증 (주민등록증/운전면허증)"
        android:textSize="12sp"/>

    <TextView
        android:layout_width="230dp"
        android:layout_height="17dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="555dp"
        android:text="2. 인증 서류 (재학증명서/ 졸업증명서/재직증명서)"
        android:textSize="12sp"/>

    <TextView
        android:layout_width="230dp"
        android:layout_height="17dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="580dp"
        android:text="3. 자격증 카테고리 선택 시 -> 자격증 증명서 필수 "
        android:textSize="12sp"/>

    <ImageView
        android:layout_width="307dp"
        android:layout_height="44dp"
        android:layout_marginTop="617dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/group513968"/>



    <android.widget.Button
        android:id="@+id/next_myself"
        android:layout_width="154dp"
        android:layout_height="32dp"
        android:layout_marginTop="680dp"
        android:layout_marginStart="122dp"
        android:background="@drawable/rectangle_yuna8787"
        android:text="다음"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

</RelativeLayout>
