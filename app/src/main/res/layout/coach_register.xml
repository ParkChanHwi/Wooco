<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Coach_registerActivity">

    <ImageView
        android:id="@+id/back"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/arrow"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/page_text_id"
        android:layout_width="110dp"
        android:layout_height="22dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="33dp"
        android:text="코치 등록하기"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="69dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="103dp"
        android:layout_height="117dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="99dp"
        android:elevation="4dp"
        android:scaleType="centerCrop"
        android:src="@drawable/efaultimage"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="247dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/some_id"
        android:layout_width="65dp"
        android:layout_height="20dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="110dp"
        android:text="이        름 :"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <EditText
        android:id="@+id/editText"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/rectangle96666"
        android:layout_toEndOf="@id/some_id"
        android:layout_alignTop="@id/some_id" />

    <TextView
        android:id="@+id/some_id2"
        android:layout_width="65dp"
        android:layout_height="20dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="143dp"
        android:text="학교/회사:"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>


    <EditText
        android:id="@+id/editText2"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/rectangle96666"
        android:layout_toEndOf="@id/some_id2"
        android:layout_alignTop="@id/some_id2" />

    <TextView
        android:id="@+id/some_id3"
        android:layout_width="65dp"
        android:layout_height="20dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="176dp"
        android:text="학과/직무:"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <EditText
        android:id="@+id/editText3"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/rectangle96666"
        android:layout_toEndOf="@id/some_id3"
        android:layout_alignTop="@id/some_id3" />

    <TextView
