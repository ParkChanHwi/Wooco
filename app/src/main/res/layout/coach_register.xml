        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>


    <Button
        android:id="@+id/addButton"
        android:layout_width="25dp"
        android:layout_height="16dp"
        android:layout_marginStart="310dp"
        android:layout_marginTop="275dp"
        android:backgroundTint="#FFFFFF"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <TextView
        android:layout_width="25dp"
        android:layout_height="16dp"
        android:layout_marginStart="310dp"
        android:layout_marginTop="275dp"
        android:text="추가"
        android:textSize="10dp"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <Button
        android:id="@+id/addButton2"
        android:layout_width="25dp"
        android:layout_height="16dp"
        android:layout_marginStart="335dp"
        android:layout_marginTop="275dp"
        android:backgroundTint="#FFFFFF"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <TextView
        android:layout_width="25dp"
        android:layout_height="16dp"
        android:layout_marginStart="335dp"
        android:layout_marginTop="275dp"
        android:text="삭제"
        android:textSize="10dp"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/coach_register1_view"
        android:layout_width="320dp"
        android:layout_height="130dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="295dp"
        android:background="@drawable/rectangle_yuna35">

    </androidx.recyclerview.widget.RecyclerView>



    <TextView
        android:id="@+id/want_category"
        android:layout_width="144dp"
        android:layout_height="18dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="448dp"
        android:text="자격증 및 기타 서류 등록"
        android:textColor="@color/white"
        android:gravity="start"
        android:paddingStart="5sp"
        android:textSize="11sp"
        android:background="@drawable/notice_rec"
        />
    <TextView
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="478dp"
        android:text="등록 방법:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:textColor="#0F4C81"/>

    <TextView
        android:layout_width="230dp"
        android:layout_height="17dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="478dp"
