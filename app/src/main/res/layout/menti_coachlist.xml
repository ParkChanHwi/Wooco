<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/some_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFFBF5">
    <RelativeLayout
        android:id="@+id/bell"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="366dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:clipToOutline="true"
        android:background="@drawable/bell"
        >
    </RelativeLayout>

    <View
        android:id="@+id/line_9"
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="85dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="65dp"
        android:background="@drawable/line_9"
        />
    <TextView
        android:id="@+id/코치목록"
        android:layout_width="72dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="174dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="26dp"
        android:text="@string/코치목록"
        android:textSize="18sp"
        android:textColor="#000000"
        android:gravity="center"
        />
    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="107dp"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <!-- 여기에 수평으로 스크롤될 요소들을 배치합니다. -->
            <android.widget.Button
                android:id="@+id/kategori"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:text="전체"
                android:textSize="12sp"
                android:layout_marginLeft="10dp"
                android:textColor="@android:color/white"
                android:background="@drawable/category_back"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />

            <android.widget.Button
                android:id="@+id/kategori1"
                android:layout_width="60dp"
                android:layout_height="35dp"
                android:text="대학교"
                android:textSize="12sp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:background="@drawable/category_back"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />

            <android.widget.Button
                android:id="@+id/kategori2"
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:text="진로/기타 고민"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:background="@drawable/rectangle_9855"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />

            <android.widget.Button
                android:id="@+id/kategori3"
                android:layout_width="76dp"
                android:layout_height="35dp"
                android:text="전공/과제"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:background="@drawable/rectangle_9855"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />

            <android.widget.Button
                android:id="@+id/kategori4"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:text="면접"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:background="@drawable/rectangle_9855"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />
            <android.widget.Button
                android:id="@+id/kategori5"
                android:layout_width="60dp"
                android:layout_height="35dp"
                android:text="자소서"
                android:textSize="12sp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:background="@drawable/rectangle_9855"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />

            <android.widget.Button
                android:id="@+id/kategori6"
                android:layout_width="60dp"
                android:layout_height="35dp"
                android:text="자격증"
                android:layout_marginLeft="10dp"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:background="@drawable/rectangle_9855"
                android:stateListAnimator="@null"
                android:paddingBottom="8dp"
                />


        </LinearLayout>
    </HorizontalScrollView>
    <RelativeLayout
        android:id="@+id/solar_star_"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="160dp"
        android:clipToOutline="true">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/solar_star_line_duotone"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/오늘의추천멘토"
        android:layout_width="76dp"
        android:layout_height="12dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="44dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="160dp"
        android:text=" 오늘의 추천 멘토 !"
        android:textSize="10sp"
        android:textColor="#000000"
        android:gravity="top"
        />
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/coachlist_recycleView"
        android:layout_width="match_parent"
        android:layout_height="486dp"
        android:layout_marginTop="181dp"/>

    <View
        android:id="@+id/line_21"
        android:layout_width="440dp"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="667dp"
        android:background="@drawable/line_9"

        />

    <ImageView
        android:id="@+id/material_sy"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="29dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="684dp"
        android:clipToOutline="true"
        android:background="@drawable/homeimg">
    </ImageView>

    <ImageView
        android:id="@+id/chat_1"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="136dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="684dp"
        android:clipToOutline="true"
        android:background="@drawable/chatimg">
    </ImageView>

    <ImageView
        android:id="@+id/uiw_date"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="239dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="682dp"
        android:clipToOutline="true"
        android:background="@drawable/calimg"
        />

    <ImageView
        android:id="@+id/group_513866"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="346dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="680dp"
        android:clipToOutline="true"
        android:background="@drawable/myimg"
        />

</RelativeLayout>