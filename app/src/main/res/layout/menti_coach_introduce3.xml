<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFBF5"
    tools:context=".Menti_coach_introduceActivity">

    <ImageView
        android:id="@+id/ArrowImageView"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/arrow"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/page_text_id"
        android:layout_width="110dp"
        android:layout_height="22dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="33dp"
        android:text="코치 소개"
        android:textSize="18sp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="69dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="99dp"
        android:elevation="4dp"
        android:scaleType="centerCrop"
        android:src="@drawable/rectangle_9534"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />
    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="99dp"
        android:elevation="4dp"
        android:scaleType="centerCrop"
        android:src="@drawable/chacharacter"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="93dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="247dp"
        android:background="@android:color/darker_gray"
        />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="65dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="158dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="98dp"
        android:text="이        름 :"
        />

    <TextView
        android:id="@+id/user_class_text2"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="223dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="98dp"
        android:textSize="18sp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/some_id2"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="158dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="131dp"
        android:text="학교 / 회사:"
        />

    <TextView
        android:id="@+id/user_class_text3"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="226dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="131dp"
        android:textSize="18sp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/some_id3"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="158dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="163dp"
        android:text="학과 / 직무:"
        />

    <TextView
        android:id="@+id/user_class_text4"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="227dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="163dp"
        android:textSize="18dp"
        android:textStyle="bold"

        />

    <TextView
        android:id="@+id/some_id4"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="158dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="194dp"
        android:text="자격증:"
        />

    <TextView
        android:id="@+id/user_class_text5"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="202dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="194dp"
        android:textSize="18dp"
        android:textStyle="bold"

        />

    <TextView
        android:id="@+id/coach_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/rectangle_9530"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:text="코치 카테고리"
        android:textColor="#A7A8A9"
        android:textStyle="bold" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/menti_coach_introduce_recycler_view"
        android:layout_width="345dp"
        android:layout_height="315dp"
        android:layout_marginTop="345dp"
        android:layout_marginStart="37dp">

    </androidx.recyclerview.widget.RecyclerView>

    <TextView
        android:id="@+id/calss_inform"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="159dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/rectangle_9530"
        android:text="수업 안내"
        android:paddingLeft="23dp"
        android:paddingTop="5dp"
        android:textStyle="bold"
        android:textColor="#A7A8A9"
        />

    <TextView
        android:id="@+id/review"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="280dp"
        android:layout_marginTop="260dp"
        android:text="수업 후기"
        android:paddingLeft="23dp"
        android:paddingTop="5dp"
        android:textStyle="bold"
        android:background="@drawable/rectangle_9538"

        />

    <TextView
        android:id="@+id/star_score"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="37dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="310dp"
        android:text="코치 평점 :"
        />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="310dp"
        android:background="@drawable/starscore"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/star_score_text"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginTop="310dp"
        android:layout_marginStart="128dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <android.widget.Button
        android:id="@+id/appointment_button"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="690dp"
        android:background="@drawable/rectangle_9532"
        android:text="예약하기"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        />

    <android.widget.Button
        android:id="@+id/consult_button"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="690dp"
        android:background="@drawable/rectangle_9532"
        android:text="상담하기"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        />

</RelativeLayout>
