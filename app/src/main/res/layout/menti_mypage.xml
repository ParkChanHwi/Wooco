<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFBF5"
    tools:context=".Menti_mypageActivity">

    <TextView
        android:id="@+id/page_text_id"
        android:layout_width="110dp"
        android:layout_height="22dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="33dp"
        android:text="마이 페이지"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="69dp"
        android:background="@android:color/darker_gray" />

    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="110dp"
        android:layout_height="117dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="100dp"
        android:elevation="4dp"
        android:scaleType="centerCrop"
        android:src="@drawable/mypage_psa"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <View
        android:layout_width="240dp"
        android:layout_height="1dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="247dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <android.widget.Button
        android:id="@+id/coach_transfer"
        android:layout_width="70dp"
        android:layout_height="23dp"
        android:layout_marginTop="14dp"
        android:layout_marginStart="330dp"
        android:text="코치 전환"
        android:textSize="12sp"
        android:textColor="@color/white"
        android:background="@drawable/rectangle_9527"/>



    <TextView
        android:id="@+id/mypage_name"
        android:layout_width="88dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="156dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="125dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textStyle="bold"/>

    <TextView
        android:layout_width="30dp"
        android:layout_height="22dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="244dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="125dp"
        android:text="회원"
        android:gravity="top"/>

    <android.widget.Button
        android:id="@+id/logout"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="126dp"
        android:text="로그아웃"
        android:textSize="10sp"
        android:background="@drawable/account_set"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="177dp"
        android:background="@drawable/group_513843">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/coin_img"
            android:layout_marginTop="4dp"
            android:layout_marginLeft="4dp"
            />
        <TextView
            android:id="@+id/coin_label"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:text="  :  "
            android:textStyle="bold"
            android:paddingLeft="3dp"
            android:textColor="@android:color/black"/>

        <TextView
            android:id="@+id/coin_count"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_toEndOf="@id/coin_label"
            android:layout_marginTop="2dp"
            android:gravity="center"
            android:paddingRight="3dp"
            android:textSize="18sp"
            android:textColor="@android:color/black"
            />
    </LinearLayout>
    <!-- 사용자가 보유한 코인 수를 표시할 TextView -->
    <TextView
        android:id="@+id/my"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="270dp"
        android:text="      MY"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="@drawable/mypage_rec"/>

    <android.widget.Button
        android:id="@+id/last_c_details"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="307dp"
        android:text="        지난 수강 내역"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null" />

    <android.widget.Button
        android:id="@+id/love_coach"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="338dp"
        android:text="        찜한 코치"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <android.widget.Button
        android:id="@+id/my_review"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="369dp"
        android:text="        별점 기록"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <TextView
        android:id="@+id/woocoin"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="410dp"
        android:text="     우코인"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="@drawable/mypage_rec"/>

    <android.widget.Button
        android:id="@+id/purchase"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="447dp"
        android:text="        구매 하기"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <android.widget.Button
        android:id="@+id/purchase_history"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="478dp"
        android:text="        구매 내역"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <TextView
        android:id="@+id/guide"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="514dp"
        android:text="     가이드"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="@drawable/mypage_rec"/>

    <android.widget.Button
        android:id="@+id/wooco_info"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="551dp"
        android:text="        우코 소개"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <android.widget.Button
        android:id="@+id/notice"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="582dp"
        android:text="        공지사항"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <android.widget.Button
        android:id="@+id/app_version"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="613dp"
        android:text="        앱 버전"
        android:textSize="18sp"
        android:gravity="start"
        android:background="#FFFBF5"
        android:stateListAnimator="@null"/>

    <View
        android:id="@+id/line_21"
        android:layout_width="440dp"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="667dp"
        android:background="@drawable/line_9"

        />

    <ImageView
        android:id="@+id/material_sy"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="29dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="684dp"
        android:clipToOutline="true"
        android:background="@drawable/homeimg_none">
    </ImageView>

    <ImageView
        android:id="@+id/chat_1"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="136dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="684dp"
        android:clipToOutline="true"
        android:background="@drawable/chatimg">
    </ImageView>

    <ImageView
        android:id="@+id/uiw_date"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="239dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="682dp"
        android:clipToOutline="true"
        android:background="@drawable/calimg"
        />

    <ImageView
        android:id="@+id/group_513866"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="346dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="680dp"
        android:clipToOutline="true"
        android:background="@drawable/myimg_none"
        />






















</RelativeLayout>
