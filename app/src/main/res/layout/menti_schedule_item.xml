<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mentischedule">

    <FrameLayout
        android:layout_width="320dp"
        android:layout_height="124dp"
        android:layout_marginTop="325dp"
        android:layout_marginStart="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:layout_width="320dp"
            android:layout_height="124dp"
            android:src="@drawable/rectangle_yuna35" />

        <TextView
            android:id="@+id/coach_name2"
            android:layout_width="100dp"
            android:layout_height="31dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="23dp"
            android:textSize="18dp"
            android:textStyle="bold"
            android:gravity="center"/>

        <TextView
            android:id="@+id/menti_name"
            android:layout_width="50dp"
            android:layout_height="31dp"
            android:layout_marginStart="115dp"
            android:layout_marginTop="23dp"
            android:textSize="18dp"
            android:textStyle="bold"
            android:text="맨티"
            android:gravity="center"/>

        <TextView
            android:id="@+id/class_day"
            android:layout_width="170dp"
            android:layout_height="15dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="60dp"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|start"
            android:src="@drawable/rectangle_yuna9" />

        <ImageView
            android:layout_width="160dp"
            android:layout_height="27dp"
            android:layout_gravity="bottom|end"
            android:src="@drawable/rectangle_yuna10" />
    </FrameLayout>
</RelativeLayout>
